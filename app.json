{
  "expo": {
    "name": "NAO Health Sync",
    "slug": "nao-health-sync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.tito.naohealthsync",
      "supportsTablet": true
    },
    "android": {
      "package": "com.tito.naohealthsync"
    },
    "plugins": [
      "react-native-health"
    ]
  }
}
